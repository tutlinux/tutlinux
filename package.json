{
  "name": "tutlinux",
  "version": "1.0.0",
  "description": "Онлайн-справочник для начинающих пользователей ОС на базе ядра Linux",
  "main": "index.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve --quiet --config=.eleventy.js",
    "start:debug": "DEBUG=tl-11ty* npx @11ty/eleventy --serve --quiet --config=.eleventy.js",
    "build": "npx @11ty/eleventy --config=.eleventy.js",
    "build:ghpages": "npx @11ty/eleventy --pathprefix=/tutlinux-ghp/ --config=.eleventy.js",
    "build:debug": "DEBUG=tl-11ty* npx @11ty/eleventy --config=.eleventy.js",
    "benchmark": "DEBUG=tl-11ty:benchmark* npx @11ty/eleventy --config=.eleventy.js",
    "commit": "git cz",
    "commit:all": "git add --all && git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tutlinux/tutlinux.git"
  },
  "keywords": [
    "tutlinux",
    "tl",
    "linux",
    "eleventyjs",
    "11ty",
    "sass",
    "vedanta",
    "sassify",
    "therteenten"
  ],
  "author": "Haba Kudzaev (therteenten)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tutlinux/tutlinux/issues"
  },
  "homepage": "https://github.com/tutlinux/tutlinux#readme",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g '.commitlint.js'"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@commitlint/cli": "^18.4.3",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7"
  }
}
